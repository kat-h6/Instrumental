<div class="container">
<h1>Leave a review</h1>

<%= simple_form_for [ @booking, @review ] do |f| %>
  <%= f.input :title %>
  <%= f.input :content %>
  <%= f.input :rating, input_html: { min: '0', max: '10', step: '1' } %>
  <%= f.submit "Submit review", class: "btn btn-primary" %>
<% end %>
</div>