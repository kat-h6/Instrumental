<h1>My Rentals</h1>
<% @bookings.each do |booking| %>
  <% @bookings = Booking.where(renter: current_user) %>
  <div>
    <%= cl_image_tag Instrument.owner %>
    <h2>Owner: <%= Instrument.user[:owner] %></h2>
    <p>Details: <%= Instrument.details %></p>
    <p>Start date: <%= Booking.start_date %></p>
    <p>End date: <%= Booking.end_date %></p>
    <p>Price: <%= Booking.price %></p>
  </div>
<% end %>

<%= link_to 'Back', index_path, class: 'btn btn-primary' %>
